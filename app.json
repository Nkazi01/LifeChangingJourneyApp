{
  "expo": {
    "name": "Life Changing Journey",
    "slug": "lifechangingjourney",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#faf5f0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "co.za.lifechangingjourney.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This app uses tracking to provide personalized wellness content.",
        "NSCameraUsageDescription": "This app may use the camera for profile photos.",
        "NSPhotoLibraryUsageDescription": "This app may access photos for profile pictures."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#faf5f0"
      },
      "edgeToEdgeEnabled": true,
      "package": "co.za.lifechangingjourney.app",
      "versionCode": 4,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "updates": {
      "enabled": false
    },
    "plugins": [
      "expo-asset",
      "expo-video",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "fe795527-0e4a-4c3e-84d5-247b756dfb83"
      }
    },
    "owner": "innovative01"
  }
}
