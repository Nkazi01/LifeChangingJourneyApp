      "package": "com.lifechangingjourney.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "updates": {
      "enabled": false
    },
    "plugins": [
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "7ecc4b99-ee2d-4693-817a-2c66784225b1"
      }
    }
